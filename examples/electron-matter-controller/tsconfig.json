{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "baseUrl": ".",
        "outDir": "dist",
        "strictNullChecks": true,
        "moduleResolution": "node16",
        "resolveJsonModule": true,
        "jsx": "react"
    }
}
